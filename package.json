{
  "devDependencies": {
    "@a2lix/symfony-collection": "^0.4.0",
    "@alpine-collective/toolkit-scroll": "^1.0.0",
    "@ryangjchandler/alpine-tooltip": "^0.2.0",
    "@symfony/webpack-encore": "^1.6.1",
    "alpinejs": "^3.3.1",
    "autoprefixer": "^10.3.4",
    "axios": "^0.21.3",
    "core-js": "^3.17.2",
    "cross-env": "^7.0.3",
    "fibers": "^5.0.0",
    "postcss": "^8.3.5",
    "postcss-loader": "^6.1.1",
    "postcss-svg": "^3.0.0",
    "prettier": "^2.3.2",
    "regenerator-runtime": "^0.13.9",
    "sass": "^1.39.0",
    "sass-loader": "^12.1.0",
    "stylelint": "^13.13.1",
    "stylelint-config-idiomatic-order": "^8.1.0",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-standard": "^22.0.0",
    "stylelint-scss": "^3.20.1",
    "tailwindcss": "2.2.9",
    "tippy.js": "^6.3.1",
    "webpack-notifier": "^1.13.0"
  },
  "license": "MIT",
  "private": true,
  "scripts": {
    "dev-server": "cross-env NODE_ENV=development encore dev-server --config ./.tools/config/webpack.config.js",
    "dev": "encore dev --config ./.tools/config/webpack.config.js",
    "watch": "encore dev --watch --config ./.tools/config/webpack.config.js",
    "build": "cross-env NODE_ENV=production encore production --progress --config ./.tools/config/webpack.config.js"
  },
  "stylelint": {
    "plugins": [
      "stylelint-scss"
    ],
    "extends": [
      "stylelint-config-idiomatic-order",
      "stylelint-config-standard",
      "stylelint-config-prettier"
    ],
    "rules": {
      "at-rule-no-unknown": [
        true,
        {
          "ignoreAtRules": [
            "apply",
            "responsive",
            "screen",
            "tailwind",
            "variants",
            "layer"
          ]
        }
      ],
      "comment-empty-line-before": null
    }
  },
  "dependencies": {
    "dropzone": "^5.9.2"
  }
}
